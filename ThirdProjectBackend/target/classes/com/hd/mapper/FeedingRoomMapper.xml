<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hd.mapper.FeedingRoomMapper">

	<select id="selectPosition" parameterType="int" resultType="com.hd.domain.FeedingRoomVO">
		select latitude, longitude
		from feeding_room
		where fno = #{fno}
	</select>
</mapper>