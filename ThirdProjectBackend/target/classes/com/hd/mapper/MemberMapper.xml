<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hd.mapper.MemberMapper">

	<insert id="insert" parameterType="com.hd.domain.MemberVO">
		insert into member(mid, password, phone)
		values (#{mid}, #{password}, #{phone})
	</insert>
	
	
</mapper>